<?xml version="1.0" encoding="UTF-8"?>


<?import de.jensd.fx.glyphs.fontawesome.FontAwesomeIconView?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TableColumn?>
<Pane xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.171" fx:controller="com.wesolemarcheweczki.frontend.controllers.AddCarrierController">
    <children>
        <VBox alignment="TOP_CENTER" layoutX="1.0" layoutY="1.0"  prefHeight="600.0" prefWidth="780.0">
            <children>
                <HBox alignment="TOP_CENTER" >
                    <TextField fx:id="carrierName" prefWidth="600" promptText="Carrier" />

                    <Button fx:id="addCarrierButton"  mnemonicParsing="false">
                        <graphic>
                            <FontAwesomeIconView glyphName="PLUS" />
                        </graphic>
                    </Button>
                </HBox>
                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Carriers">
                    <font>
                        <Font size="24.0" />
                    </font>
                </Text>
                <TableView fx:id="dataTable" >
                    <columns>
                        <TableColumn fx:id="carrierColumn" prefWidth="800.0" text="Carrier" />
                    </columns>
                </TableView>
            </children>
        </VBox>
    </children>
</Pane>